{
    "QWidget": {
      "background-color": "#2E3440",
      "color": "#D8DEE9",
      "font-family": "Arial, sans-serif",
      "font-size": "14px",
      "border-radius": "10px",
      "padding": "10px"
    },
    "QLabel": {
      "color": "#D8DEE9",
      "font-size": "16px"
    },
    "QTabWidget::pane": {
      "border": "2px solid #4C566A",
      "border-radius": "10px"
    },
    "QTabWidget::tab-bar": {
      "alignment": "center"
    },
    "QTabWidget::tab": {
      "background-color": "#3B4252",
      "color": "#D8DEE9",
      "padding": "15px",
      "border-radius": "10px",
      "font-size": "18px"
    },
    "QTabWidget::tab:selected": {
      "background-color": "#4C566A",
      "border-bottom": "3px solid #81A1C1",
      "font-size": "20px"
    },
    "QPushButton": {
      "background-color": "#5E81AC",
      "color": "white",
      "padding": "15px",
      "border": "2px solid #4C566A",
      "border-radius": "10px",
      "font-size": "18px"
    },
    "QPushButton:hover": {
      "background-color": "#4C719A"
    },
    "QLineEdit": {
      "background-color": "#3B4252",
      "color": "#D8DEE9",
      "border": "2px solid #4C566A",
      "border-radius": "8px",
      "padding": "12px",
      "font-size": "16px"
    },
    "QComboBox": {
      "background-color": "#3B4252",
      "color": "#D8DEE9",
      "border": "2px solid #4C566A",
      "border-radius": "8px",
      "padding": "12px",
      "font-size": "16px"
    },
    "QComboBox::drop-down": {
      "border": "none"
    },
    "QListWidget": {
      "background-color": "#3B4252",
      "color": "#D8DEE9",
      "border": "2px solid #4C566A",
      "border-radius": "8px",
      "padding": "12px",
      "font-size": "16px"
    },
    "QListWidget::item": {
      "padding": "15px"
    },
    "QListWidget::item:selected": {
      "background-color": "#81A1C1",
      "color": "white"
    },
    "QPushButton:disabled": {
      "background-color": "#4C566A",
      "color": "#D8DEE9"
    },
    "QScrollArea": {
      "background-color": "transparent"
    },
    "QGridLayout": {
      "margin": "15px"
    },
    "QHBoxLayout": {
      "spacing": "15px"
    },
    "QVBoxLayout": {
      "spacing": "15px"
    },
    "QWidget[background-color=\"transparent\"]": {
      "background-color": "transparent"
    },
    "QScrollBar:horizontal": {
    "border": "1px solid #434C5E",
    "background": "#2E3440",
    "height": "15px",
    "margin": "0px 0px 0 0px"
  },
  "QScrollBar:vertical": {
    "border": "1px solid #434C5E",
    "background": "#2E3440",
    "width": "15px",
    "margin": "0px 0px 0px 0px"
  },
  "QScrollBar::handle:horizontal": {
    "background": "#88C0D0",
    "min-width": "20px",
    "border-radius": "4px",
    "border": "2px solid #88C0D0"
  },
  "QScrollBar::handle:vertical": {
    "background": "#88C0D0",
    "min-height": "20px",
    "border-radius": "4px",
    "border": "2px solid #88C0D0"
  },
  "QScrollBar::add-line:horizontal": {
    "background": "#5E81AC",
    "width": "16px",
    "subcontrol-position": "right",
    "subcontrol-origin": "margin",
    "border": "1px solid #434C5E"
  },
  "QScrollBar::add-line:vertical": {
    "background": "#5E81AC",
    "height": "16px",
    "subcontrol-position": "bottom",
    "subcontrol-origin": "margin",
    "border": "1px solid #434C5E"
  },
  "QScrollBar::sub-line:horizontal": {
    "background": "#81A1C1",
    "width": "16px",
    "subcontrol-position": "left",
    "subcontrol-origin": "margin",
    "border": "1px solid #434C5E"
  },
  "QScrollBar::sub-line:vertical": {
    "background": "#81A1C1",
    "height": "16px",
    "subcontrol-position": "top",
    "subcontrol-origin": "margin",
    "border": "1px solid #434C5E"
  },
  "QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal": {
    "width": "4px",
    "height": "4px",
    "background": "#88C0D0"
  },
  "QScrollBar:up-arrow:vertical, QScrollBar::down-arrow:vertical": {
    "width": "4px",
    "height": "4px",
    "background": "#88C0D0"
  },
  "QScrollBar::add-page:horizontal": {
    "background": "none"
  },
  "QScrollBar::sub-page:horizontal": {
    "background": "none"
  },
  "QScrollBar::add-page:vertical": {
    "background": "none"
  },
  "QScrollBar::sub-page:vertical": {
    "background": "none"
  }
    
  }
  